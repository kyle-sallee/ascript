.SH SYNOPSIS
.EX
.TS
llll.
\fBargot	direct	direct	task\fR
go 2	"far"	'near'	At label interpret.
.TE

.SH EXAMPLE
.EX
.ta T 8n
.in -8
\fB
io
trek
go 2		"logger"	'selfy'
"logger",	out now		`This line never  outputs.\\n`,	ret
\&'selfy',	out now 	`This line always outputs.\\n`,	ret
\fR
.in

